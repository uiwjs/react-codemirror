{
  "name": "@uiw/react-codemirror",
  "version": "4.7.0",
  "description": "CodeMirror component for React.",
  "homepage": "https://uiwjs.github.io/react-codemirror",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "scripts": {
    "prepare": "husky install && npm run build",
    "watch": "tsbb watch",
    "build": "tsbb build",
    "bundle": "ncc build src/index.tsx --target web --filename codemirror",
    "bundle:min": "ncc build src/index.tsx --target web --filename codemirror --minify",
    "test": "tsbb test --env=jsdom",
    "coverage": "tsbb test --coverage --bail",
    "doc": "kkt build --app-src ./website",
    "start": "kkt start --app-src ./website",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,html,less,md,json}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-codemirror.git"
  },
  "keywords": [
    "react",
    "codemirror",
    "codemirror6",
    "react-codemirror",
    "editor",
    "syntax",
    "ide",
    "code"
  ],
  "files": [
    "dist",
    "src",
    "esm",
    "cjs"
  ],
  "author": "kenny wong <wowohoo@qq.com>",
  "license": "MIT",
  "jest": {
    "coverageReporters": [
      "lcov",
      "json-summary"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.11.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@babel/runtime": ">=7.11.0",
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/state": "^6.0.0",
    "@codemirror/theme-one-dark": "^6.0.0",
    "@codemirror/view": "^6.0.0"
  },
  "devDependencies": {
    "@codemirror/lang-cpp": "~6.0.0",
    "@codemirror/lang-html": "~6.0.0",
    "@codemirror/lang-java": "~6.0.0",
    "@codemirror/lang-javascript": "~6.0.0",
    "@codemirror/lang-json": "~6.0.0",
    "@codemirror/lang-lezer": "~6.0.0",
    "@codemirror/lang-markdown": "~6.0.0",
    "@codemirror/lang-php": "~6.0.0",
    "@codemirror/lang-python": "~6.0.0",
    "@codemirror/lang-rust": "~6.0.0",
    "@codemirror/lang-sql": "~6.0.0",
    "@codemirror/lang-xml": "~6.0.0",
    "@codemirror/language": "6.0.0",
    "@codemirror/legacy-modes": "~6.0.0",
    "@kkt/less-modules": "~7.1.1",
    "@kkt/ncc": "~1.0.8",
    "@kkt/raw-modules": "~7.1.1",
    "@kkt/scope-plugin-options": "~7.1.1",
    "@testing-library/react": "^13.0.0",
    "@types/react": "~18.0.1",
    "@types/react-dom": "~18.0.0",
    "@types/react-test-renderer": "~18.0.0",
    "@uiw/react-github-corners": "~1.5.14",
    "@uiw/react-markdown-preview": "~4.0.5",
    "@uiw/react-shields": "~1.1.3",
    "@uiw/reset.css": "~1.0.5",
    "@wcj/dark-mode": "~1.0.12",
    "code-example": "~3.3.1",
    "husky": "~7.0.4",
    "kkt": "~7.1.5",
    "lint-staged": "~12.4.0",
    "prettier": "~2.6.0",
    "react": "~18.0.0",
    "react-dom": "~18.0.0",
    "react-test-renderer": "~18.0.0",
    "tsbb": "~3.7.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
