{
  "name": "@uiw/react-codemirror",
  "version": "4.0.6",
  "description": "CodeMirror component for React.",
  "homepage": "https://uiwjs.github.io/react-codemirror",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "scripts": {
    "prepare": "husky install && npm run build",
    "watch": "tsbb watch",
    "build": "tsbb build",
    "test": "tsbb test --env=jsdom",
    "coverage": "tsbb test --coverage",
    "doc": "kkt build --app-src ./website",
    "start": "kkt start --app-src ./website",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,html,less,md,json}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-codemirror.git"
  },
  "keywords": [
    "react",
    "codemirror",
    "codemirror6",
    "react-codemirror",
    "editor",
    "syntax",
    "ide",
    "code"
  ],
  "files": [
    "src",
    "esm",
    "cjs"
  ],
  "author": "kenny wong <wowohoo@qq.com>",
  "license": "MIT",
  "peerDependencies": {
    "@babel/runtime": ">=7.11.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "7.15.4",
    "@codemirror/basic-setup": "0.19.0",
    "@codemirror/state": "0.19.2",
    "@codemirror/theme-one-dark": "0.19.0",
    "@codemirror/view": "0.19.9"
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "json-summary"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,less,md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@codemirror/lang-cpp": "0.19.1",
    "@codemirror/lang-html": "0.19.3",
    "@codemirror/lang-java": "0.19.1",
    "@codemirror/lang-javascript": "0.19.2",
    "@codemirror/lang-json": "0.19.1",
    "@codemirror/lang-lezer": "0.19.1",
    "@codemirror/lang-markdown": "0.19.1",
    "@codemirror/lang-php": "0.19.1",
    "@codemirror/lang-python": "0.19.2",
    "@codemirror/lang-rust": "0.19.1",
    "@codemirror/lang-sql": "0.19.3",
    "@codemirror/lang-xml": "0.19.2",
    "@codemirror/legacy-modes": "0.19.0",
    "@codemirror/stream-parser": "0.19.2",
    "@kkt/less-modules": "6.11.0",
    "@kkt/raw-modules": "6.11.0",
    "@kkt/scope-plugin-options": "6.11.0",
    "@types/react": "17.0.26",
    "@types/react-dom": "17.0.9",
    "@types/react-test-renderer": "17.0.1",
    "@uiw/react-github-corners": "1.5.3",
    "@uiw/react-markdown-preview": "3.3.5",
    "@uiw/react-shields": "1.1.2",
    "@uiw/reset.css": "1.0.5",
    "rehype-attr": "2.0.3",
    "code-example": "3.3.1",
    "husky": "7.0.2",
    "jest": "27.2.2",
    "kkt": "6.11.0",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "tsbb": "3.3.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
